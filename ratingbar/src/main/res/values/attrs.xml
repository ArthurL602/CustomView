<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingBar">
        <!--非选中的图形id-->
        <attr name="starNormal" format="reference"/>
        <!--选中的图形id-->
        <attr name="starFocus" format="reference"/>
        <!--图形数量-->
        <attr name="startNums" format="integer"/>
        <!--focus图形数量-->
        <attr name="focusNums" format="integer"/>
        <!--图形之间的间隔-->
        <attr name="starPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CStarView">
        <attr name="starNum" format="integer"/>
        <attr name="defaultStar" format="reference"/>
        <attr name="selectedStar" format="reference"/>
        <attr name="starPadding" format="dimension|reference"/>
        <attr name="starWidth" format="dimension|reference"/>
        <attr name="starHeight" format="reference|dimension"/>
    </declare-styleable>
</resources>